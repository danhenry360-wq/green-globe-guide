{
  "installCommand": "pnpm install --no-frozen-lockfile",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.budquest.guide"
        }
      ],
      "destination": "https://budquest.guide/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}